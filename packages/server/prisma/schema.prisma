generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  vkId      String   @unique
  vkData    Json?
  createdAt DateTime @default(now()) @db.Timestamptz(3)
}
